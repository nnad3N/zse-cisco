{
  "extends": ["next/core-web-vitals", "prettier", "plugin:prettier/recommended"],
  "plugins": ["prettier"],
  "rules": {
    "jsx-a11y/alt-text": 0,
    "@next/next/no-document-import-in-page": 0,
    "prettier/prettier": [
      "warn",
      {
        "singleQuote": true,
        "endOfLine": "auto",
        "printWidth": 150
      }
    ]
  }
}
