{
  "extends": ["next", "prettier", "plugin:prettier/recommended", "babel"],
  "plugins": ["prettier, import"],
  "rules": {
    "import/no-unresolved": "error",
    "prettier/prettier": [
      "warn",
      {
        "singleQuote": true,
        "endOfLine": "auto",
        "printWidth": 150
      }
    ]
  }
}
